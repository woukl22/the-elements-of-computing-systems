/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Nand(a=a[0], b=b[0], out=n0);
    Nand(a=a[1], b=b[1], out=n1);
    Nand(a=a[2], b=b[2], out=n2);
    Nand(a=a[3], b=b[3], out=n3);
    Nand(a=a[4], b=b[4], out=n4);
    Nand(a=a[5], b=b[5], out=n5);
    Nand(a=a[6], b=b[6], out=n6);
    Nand(a=a[7], b=b[7], out=n7);
    Nand(a=a[8], b=b[8], out=n8);
    Nand(a=a[9], b=b[9], out=n9);
    Nand(a=a[10], b=b[10], out=n10);
    Nand(a=a[11], b=b[11], out=n11);
    Nand(a=a[12], b=b[12], out=n12);
    Nand(a=a[13], b=b[13], out=n13);
    Nand(a=a[14], b=b[14], out=n14);
    Nand(a=a[15], b=b[15], out=n15);
    Nand(a=n0, b=n0, out=out[0]);
    Nand(a=n1, b=n1, out=out[1]);
    Nand(a=n2, b=n2, out=out[2]);
    Nand(a=n3, b=n3, out=out[3]);
    Nand(a=n4, b=n4, out=out[4]);
    Nand(a=n5, b=n5, out=out[5]);
    Nand(a=n6, b=n6, out=out[6]);
    Nand(a=n7, b=n7, out=out[7]);
    Nand(a=n8, b=n8, out=out[8]);
    Nand(a=n9, b=n9, out=out[9]);
    Nand(a=n10, b=n10, out=out[10]);
    Nand(a=n11, b=n11, out=out[11]);
    Nand(a=n12, b=n12, out=out[12]);
    Nand(a=n13, b=n13, out=out[13]);
    Nand(a=n14, b=n14, out=out[14]);
    Nand(a=n15, b=n15, out=out[15]);
}